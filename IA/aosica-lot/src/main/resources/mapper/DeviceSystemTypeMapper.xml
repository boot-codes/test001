  <insert id="insert" parameterType="com.aosica.lot.entity.DeviceSystemType">
    insert into t_device_system_type (id, project_id, system_type_name, 
      system_type_code, create_time, creater, 
      update_time, updater)
    values (#{id,jdbcType=BIGINT}, #{projectId,jdbcType=BIGINT}, #{systemTypeName,jdbcType=VARCHAR}, 
      #{systemTypeCode,jdbcType=VARCHAR}, #{createTime,jdbcType=TIMESTAMP}, #{creater,jdbcType=BIGINT}, 
      #{updateTime,jdbcType=TIMESTAMP}, #{updater,jdbcType=BIGINT})
  </insert>


  <select id="getByDtoCondition" resultMap="BaseResultMap" parameterType="com.aosica.lot.dto.DeviceSystemTypeQueryDTO">

        select
            <include refid="Base_Column_List" />
        from t_device_system_type
        <where>
          <if test="dto.id != null">
            id=#{dto.id}
          </if>

          <if test="dto.systemTypeName != null">
            and systemTypeName =#{dto.systemTypeName}
          </if>

          <if test="dto.systemTypeCode != null">
            and systemTypeCode =#{dto.systemTypeCode}
          </if>

        </where>

        <if test="dto.orderByClause != null">
          order by ${dto.orderByClause}
        </if>

  </select>



  <!-- 查询所有的系统类型 -->
  <select id="getAllList" resultType="com.aosica.lot.entity.DeviceSystemType">
